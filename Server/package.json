{
  "name": "server",
  "version": "1.0.0",
  "description": "A Node.js server to handle communication with the [Recognition System](../Recognition%20System/README.md \"Recognition System Project\") and the [Mobile Application](../Mobile%20Application/README.md \"Mobile Application Project\").",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --ignore './storage/' --exec babel-node src/index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "test": "jest --verbose --detectOpenHandles"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "child_process": "^1.0.2",
    "expo-server-sdk": "^3.5.0",
    "express": "^4.17.3",
    "express-validator": "^6.5.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.13",
    "sequelize-cli": "^5.5.1",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.3",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.10.2",
    "cross-env": "^7.0.2",
    "jest": "^26.4.0",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
